{
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/phplrt"
    },
    "name": "phplrt/phplrt",
    "type": "library",
    "description": "PHP Language Recognition Tool",
    "keywords": ["compiler", "parser", "ast", "trace", "llk", "ll1", "lookahead", "buffer", "grammar"],
    "homepage": "https://github.com/phplrt",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-spl": "*",
        "symfony/polyfill-php73": "~1.12",
        "zendframework/zend-code": "~3.0"
    },
    "require-dev": {
        "brianium/paratest": "^2.1",
        "illuminate/support": "~6.0",
        "laravel/helpers": "~1.1",
        "monolog/monolog": "~1.0",
        "phpunit/phpunit": "^7.5",
        "psr/http-message": "~1.0",
        "squizlabs/php_codesniffer": "~3.0",
        "symfony/var-dumper": "~3.4|~4.0",
        "symplify/monorepo-builder": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Compiler\\": "src/Compiler/src",
            "Phplrt\\Contracts\\Ast\\": "src/Contracts/Ast/src/",
            "Phplrt\\Contracts\\Lexer\\": "src/Contracts/Lexer/src/",
            "Phplrt\\Contracts\\Parser\\": "src/Contracts/Parser/src/",
            "Phplrt\\Contracts\\Source\\": "src/Contracts/Source/src/",
            "Phplrt\\Lexer\\": "src/Lexer/src",
            "Phplrt\\Parser\\": "src/Parser/src",
            "Phplrt\\Position\\": "src/Position/src",
            "Phplrt\\Source\\": "src/Source/src",
            "Phplrt\\Visitor\\": "src/Visitor/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Tests\\": "tests"
        }
    },
    "replace": {
        "phplrt/lexer": "self.version",
        "phplrt/parser": "self.version",
        "phplrt/position": "self.version",
        "phplrt/visitor": "self.version",
        "phplrt/ast-contracts": "self.version",
        "phplrt/compiler": "self.version",
        "phplrt/lexer-contracts": "self.version",
        "phplrt/parser-contracts": "self.version",
        "phplrt/source": "self.version",
        "phplrt/source-contracts": "self.version"
    },
    "provide": {
        "phplrt/lexer-contracts-implementation": "~3.0",
        "phplrt/parser-contracts-implementation": "~3.0",
        "phplrt/source-contracts-implementation": "~3.0"
    },
    "scripts": {
        "phpcbf": [
            "@php vendor/squizlabs/php_codesniffer/bin/phpcbf -p --standard=PSR12 src"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
