{
    "name": "phplrt/dumper",
    "type": "library",
    "description": "The phplrt abstract syntax tree dumper package",
    "license": "MIT",
    "keywords": [
        "dump",
        "dumper",
        "debug"
    ],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/dumper"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "phplrt/dumper-contracts": "2.0.*|2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Dumper\\": ""
        }
    },
    "provide": {
        "phplrt/dumper-implementation": "2.0.0"
    },
    "suggest": {
        "ext-dom": "Dumping abstract syntax tree as XML"
    },
    "extra": {
        "branch-alias": {
            "master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
