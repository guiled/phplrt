{
    "name": "phplrt/contracts-exception",
    "type": "library",
    "description": "A set of phplrt exceptions abstractions",
    "license": "MIT",
    "keywords": [
        "abstractions",
        "contracts",
        "decoupling",
        "interfaces",
        "interoperability",
        "standards",
        "exception",
        "formatting"
    ],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/contracts-exception"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "phplrt/contracts-io": "2.0.*|2.0.x-dev",
        "phplrt/contracts-position": "2.0.*|2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Contracts\\Exception\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
